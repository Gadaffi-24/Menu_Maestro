        "foregroundImage": "./logo.jpg",
        "backgroundColor": "#01e6ff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./logo.jpg"
    }
  }
}
